#include <stdio.h>

int main() {
    int n = 10;
    int first = 0, second = 1, next;

    printf("Fibonacci Series for first %d numbers:\n", n);

    for (int i = 0; i < n; i++) {
        printf("%d, ", first);
        next = first + second;
        first = second;
        second = next;
    }

    return 0;
}
